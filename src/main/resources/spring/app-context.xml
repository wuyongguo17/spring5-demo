<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="provider" class="com.wyg.spring5.demo1.inter.impl.HelloWorldMessageProvider"></bean>
    <bean id="render" class="com.wyg.spring5.demo1.inter.impl.StandarOutMessageRenderer" p:messageProvider-ref="provider"></bean>

</beans>